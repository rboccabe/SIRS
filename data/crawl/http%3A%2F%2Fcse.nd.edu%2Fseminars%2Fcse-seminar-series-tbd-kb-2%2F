        <div class="documentFirstHeading seminarHeader">
            <h1>Evangelos Papalexakis - Big Signal Processing for Multi-Aspect Data Science</h1>
            <!--<div tal:condition="description" tal:content="description">middle</div>-->
        </div>

        <div class="eventInfo">
            <div class="container">
